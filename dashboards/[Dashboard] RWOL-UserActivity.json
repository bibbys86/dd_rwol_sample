                "type": "group",
                "layout_type": "ordered",
                "widgets": [
                    {
                        "id": 1116043179991225,
                        "definition": {
                            "title": "Views by Measure Metric",
                            "title_size": "16",
                            "title_align": "left",
                            "show_legend": true,
                            "legend_layout": "horizontal",
                            "legend_columns": [
                                "avg",
                                "min",
                                "max",
                                "value",
                                "sum"
                            ],
                            "time": {},
                            "type": "timeseries",
                            "requests": [
                                {
                                    "formulas": [
                                        {
                                            "number_format": {
                                                "unit": {
                                                    "type": "canonical_unit",
                                                    "unit_name": "view"
                                                }
                                            },
                                            "formula": "default_zero(trafficChanges)"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "data_source": "metrics",
                                            "name": "trafficChanges",
                                            "query": "sum:rum.measure.view{*} by {view.name}.as_count()",
                                            "aggregator": "sum"
                                        }
                                    ],
                                    "response_format": "timeseries",
                                    "style": {
                                        "order_by": "values",
                                        "palette": "classic",
                                        "line_type": "solid",
                                        "line_width": "normal"
                                    },
                                    "display_type": "bars"
                                }
                            ]
                        },
                        "layout": {
                            "x": 0,
                            "y": 0,
                            "width": 6,
                            "height": 4
                        }
                    },
                    {
                        "id": 4471230137152128,
                        "definition": {
                            "title": "Views by Session",
                            "title_size": "16",
                            "title_align": "left",
                            "show_legend": true,
                            "legend_layout": "horizontal",
                            "legend_columns": [
                                "avg",
                                "min",
                                "max",
                                "value",
                                "sum"
                            ],
                            "time": {},
                            "type": "timeseries",
                            "requests": [
                                {
                                    "formulas": [
                                        {
                                            "alias": "Sessions",
                                            "formula": "query3"
                                        }
                                    ],
                                    "queries": [
                                        {
                                            "name": "query3",
                                            "data_source": "rum",
                                            "search": {
                                                "query": "@type:view $service $version $applicationId $country $device $OS $browser $env $sessionType"
                                            },
                                            "indexes": [
                                                "*"
                                            ],
                                            "group_by": [
                                                {
                                                    "facet": "@view.name",
                                                    "limit": 10,
                                                    "sort": {
                                                        "aggregation": "count",
                                                        "order": "desc",
                                                        "metric": "count"
                                                    }
                                                }
                                            ],
                                            "compute": {
                                                "aggregation": "count"
                                            },
                                            "storage": "hot"
                                        }
                                    ],
                                    "response_format": "timeseries",
                                    "style": {
                                        "palette": "purple",
                                        "line_type": "solid",
                                        "line_width": "normal"
                                    },
                                    "display_type": "bars"
                                }
                            ],
                            "yaxis": {
                                "scale": "linear",
                                "label": "",
                                "include_zero": true,
                                "min": "auto",
                                "max": "auto"
                            },
                            "markers": []
                        },
                        "layout": {
                            "x": 6,
                            "y": 0,
                            "width": 6,
                            "height": 4
                        }
                    }
                ]
            },

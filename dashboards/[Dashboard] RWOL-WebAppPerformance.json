                    {
                        "id": 1167310462324884,
                        "definition": {
                            "title": "Performance overview of the most popular pages",
                            "title_size": "16",
                            "title_align": "left",
                            "type": "query_table",
                            "requests": [
                                {
                                    "response_format": "scalar",
                                    "queries": [
                                        {
                                            "data_source": "metrics",
                                            "name": "query1",
                                            "query": "sum:rum.measure.view{*} by {view.name}.as_count()",
                                            "aggregator": "sum"
                                        },
                                        {
                                            "data_source": "metrics",
                                            "name": "query2",
                                            "query": "avg:rum.measure.view.loading_time{*} by {view.name}",
                                            "aggregator": "avg"
                                        },
                                        {
                                            "data_source": "metrics",
                                            "name": "query3",
                                            "query": "avg:rum.measure.view.largest_contentful_paint{*} by {view.name}",
                                            "aggregator": "avg"
                                        },
                                        {
                                            "data_source": "metrics",
                                            "name": "query4",
                                            "query": "avg:rum.measure.view.interaction_to_next_paint{*} by {view.name}",
                                            "aggregator": "avg"
                                        },
                                        {
                                            "data_source": "metrics",
                                            "name": "query5",
                                            "query": "avg:rum.measure.view.first_contentful_paint{*} by {view.name}",
                                            "aggregator": "avg"
                                        },
                                        {
                                            "data_source": "metrics",
                                            "name": "query6",
                                            "query": "avg:rum.measure.view.cumulative_layout_shift{*} by {view.name}",
                                            "aggregator": "avg"
                                        }
                                    ],
                                    "sort": {
                                        "order_by": [
                                            {
                                                "type": "group",
                                                "name": "view.name",
                                                "order": "asc"
                                            }
                                        ],
                                        "count": 500
                                    },
                                    "formulas": [
                                        {
                                            "alias": "View Count",
                                            "cell_display_mode": "bar",
                                            "formula": "query1"
                                        },
                                        {
                                            "alias": "LT",
                                            "cell_display_mode": "number",
                                            "formula": "query2"
                                        },
                                        {
                                            "alias": "LCP",
                                            "cell_display_mode": "number",
                                            "conditional_formats": [
                                                {
                                                    "palette": "red_on_white",
                                                    "comparator": ">",
                                                    "value": 4000000000
                                                },
                                                {
                                                    "palette": "yellow_on_white",
                                                    "comparator": ">",
                                                    "value": 2500000000
                                                },
                                                {
                                                    "palette": "green_on_white",
                                                    "comparator": "<=",
                                                    "value": 2500000000
                                                }
                                            ],
                                            "formula": "query3"
                                        },
                                        {
                                            "alias": "INP",
                                            "cell_display_mode": "number",
                                            "conditional_formats": [
                                                {
                                                    "palette": "red_on_white",
                                                    "comparator": ">",
                                                    "value": 500000000
                                                },
                                                {
                                                    "palette": "yellow_on_white",
                                                    "comparator": ">",
                                                    "value": 200000000
                                                },
                                                {
                                                    "palette": "green_on_white",
                                                    "comparator": "<=",
                                                    "value": 200000000
                                                }
                                            ],
                                            "formula": "query4"
                                        },
                                        {
                                            "alias": "FCP",
                                            "cell_display_mode": "number",
                                            "conditional_formats": [
                                                {
                                                    "palette": "red_on_white",
                                                    "comparator": ">",
                                                    "value": 0.25
                                                },
                                                {
                                                    "palette": "yellow_on_white",
                                                    "comparator": ">",
                                                    "value": 0.1
                                                },
                                                {
                                                    "palette": "green_on_white",
                                                    "comparator": "<=",
                                                    "value": 0.1
                                                }
                                            ],
                                            "formula": "query5"
                                        },
                                        {
                                            "alias": "CLS",
                                            "cell_display_mode": "number",
                                            "conditional_formats": [
                                                {
                                                    "palette": "red_on_white",
                                                    "comparator": ">",
                                                    "value": 0.25
                                                },
                                                {
                                                    "palette": "yellow_on_white",
                                                    "comparator": ">",
                                                    "value": 0.1
                                                },
                                                {
                                                    "palette": "green_on_white",
                                                    "comparator": "<=",
                                                    "value": 0.1
                                                }
                                            ],
                                            "formula": "query6"
                                        }
                                    ]
                                }
                            ],
                            "has_search_bar": "never",
                            "custom_links": [
                                {
                                    "link": "/dash/integration/rum_errors_browser?tpl_var_viewName={{@view.name.value}}",
